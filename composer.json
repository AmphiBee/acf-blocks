{
    "name": "amphibee/acf-blocks",
    "description": "Provide abstracted class to help build ACF-based Gutenberg Blocks",
    "type": "library",
    "require": {
        "wordplate/acf": "^11.2",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "preferred-install": "dist"
    },
    "authors": [
        {
            "name": "AmphiBee",
            "email": "dev@amphibee.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "AmphiBee\\AcfBlocks\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
