{
    "name": "amphibee/acf-blocks",
    "description": "Provide abstracted class to help build ACF-based Gutenberg Blocks",
    "type": "library",
    "require": {
        "wordplate/acf": "^12.0",
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.21",
        "squizlabs/php_codesniffer": "3.7.1"
    },
    "config": {
        "preferred-install": "dist"
    },
    "authors": [
        {
            "name": "AmphiBee",
            "email": "dev@amphibee.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "AmphiBee\\AcfBlocks\\": "src/"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
